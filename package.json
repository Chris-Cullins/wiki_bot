{
  "name": "wiki-bot",
  "version": "0.1.0",
  "description": "AI Agent powered wiki documentation generator for GitHub repositories",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && mkdir -p dist/prompts dist/templates && cp src/prompts/*.md dist/prompts/ && cp -R src/templates/. dist/templates/",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "wiki",
    "documentation",
    "ai",
    "github",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.38.0",
    "dotenv": "^16.4.5",
    "ignore": "^7.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
